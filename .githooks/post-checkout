curl "https://rtp-opensource.oss-cn-hangzhou.aliyuncs.com/rtp_llm/test/model_test/fake_test/testdata/llama/fake/hf_source/tokenizer.model" -o rtp_llm/test-data/rtp_llm/test/model_test/fake_test/testdata/llama/fake/hf_source/tokenizer.model &
mkdir -p rtp_llm/test-data/rtp_llm/test/model_test/fake_test/testdata/qwen_0.5b/
curl "https://rtp-opensource.oss-cn-hangzhou.aliyuncs.com/rtp_llm/test/model_test/fake_test/testdata/qwen_0.5b/layer_0.pt" -o rtp_llm/test-data/rtp_llm/test/model_test/fake_test/testdata/qwen_0.5b/layer_0.pt &
curl "https://rtp-opensource.oss-cn-hangzhou.aliyuncs.com/rtp_llm/test/model_test/fake_test/testdata/qwen_0.5b/layer_1.pt" -o rtp_llm/test-data/rtp_llm/test/model_test/fake_test/testdata/qwen_0.5b/layer_1.pt &
curl "https://rtp-opensource.oss-cn-hangzhou.aliyuncs.com/rtp_llm/test/model_test/fake_test/testdata/qwen_0.5b/layer_2.pt" -o rtp_llm/test-data/rtp_llm/test/model_test/fake_test/testdata/qwen_0.5b/layer_2.pt
curl "https://rtp-opensource.oss-cn-hangzhou.aliyuncs.com/rtp_llm/test/model_test/fake_test/testdata/qwen_0.5b/layer_3.pt" -o rtp_llm/test-data/rtp_llm/test/model_test/fake_test/testdata/qwen_0.5b/layer_3.pt &
curl "https://rtp-opensource.oss-cn-hangzhou.aliyuncs.com/rtp_llm/test/model_test/fake_test/testdata/qwen_0.5b/layer_4.pt" -o rtp_llm/test-data/rtp_llm/test/model_test/fake_test/testdata/qwen_0.5b/layer_4.pt &
mkdir -p rtp_llm/test-data/rtp_llm/test/tokenizer_test/testdata/chatglm3_tokenizer
curl "https://rtp-opensource.oss-cn-hangzhou.aliyuncs.com/rtp_llm/test/tokenizer_test/testdata/chatglm3_tokenizer/tokenizer.model" -o rtp_llm/test-data/rtp_llm/test/tokenizer_test/testdata/chatglm3_tokenizer/tokenizer.model &
mkdir -p rtp_llm/test-data/rtp_llm/utils/test/testdata/ckpt_database_testdata/bin_testdata
curl "https://rtp-opensource.oss-cn-hangzhou.aliyuncs.com/rtp_llm/utils/test/testdata/ckpt_database_testdata/bin_testdata/pytorch_model.bin" -o rtp_llm/test-data/rtp_llm/utils/test/testdata/ckpt_database_testdata/bin_testdata/pytorch_model.bin &
