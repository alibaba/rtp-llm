curl "https://rtp-opensource.oss-cn-hangzhou.aliyuncs.com/rtp_llm/utils/test/testdata/ckpt_database_testdata/lora_testdata/adapter_model.bin" -o rtp_llm/test-data/rtp_llm/utils/test/testdata/ckpt_database_testdata/lora_testdata/adapter_model.bin &
mkdir -p rtp_llm/test-data/rtp_llm/utils/test/testdata/ckpt_database_testdata/lora_testdata_safetensor
curl "https://rtp-opensource.oss-cn-hangzhou.aliyuncs.com/rtp_llm/utils/test/testdata/ckpt_database_testdata/lora_testdata_safetensor/adapter_model.safetensors" -o rtp_llm/test-data/rtp_llm/utils/test/testdata/ckpt_database_testdata/lora_testdata_safetensor/adapter_model.safetensors &
mkdir -p rtp_llm/test-data/rtp_llm/utils/test/testdata/ckpt_database_testdata/pt_testdata
curl "https://rtp-opensource.oss-cn-hangzhou.aliyuncs.com/rtp_llm/utils/test/testdata/ckpt_database_testdata/pt_testdata/test.pt" -o rtp_llm/test-data/rtp_llm/utils/test/testdata/ckpt_database_testdata/pt_testdata/test.pt &
mkdir -p rtp_llm/test-data/rtp_llm/utils/test/testdata/ckpt_database_testdata/safetensor_testdata
curl "https://rtp-opensource.oss-cn-hangzhou.aliyuncs.com/rtp_llm/utils/test/testdata/ckpt_database_testdata/safetensor_testdata/test.safetensors" -o rtp_llm/test-data/rtp_llm/utils/test/testdata/ckpt_database_testdata/safetensor_testdata/test.safetensors &
wait